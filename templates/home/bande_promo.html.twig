<div class="high-promo flex">
	<div class="promo">
		<div class="carouselSelect" data-pagination=true data-visible={{bande.slideVisible}} data-scroll={{bande.slideToScroll}}>
			{% for promo in bande.bandePromo.promos %}
				<div class="item_promo">
					<div class="item_image">
						<img width="100%" height="100%" src="{{ vich_uploader_asset(promo.picture,'imageFile') }}">
					</div>
				</div>
			{% endfor %}
		</div>
	</div>
</div>
