<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}
				Welcome!
			{% endblock %}
		</title>

		{% block cdn %}
			<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
		{% endblock %}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app')}}
		{% endblock %}


	</head>
	<body>
		{% block header %}
		{{ render(controller(
			        'App\\Controller\\RecycleController::header'
			    )) }}
			{# {{ include('partials/_header.html.twig') }} #}
		{% endblock %}
		{% block body %}
			je suis le body
		{% endblock %}
		{% for cat, list in app.flashes %}
			{% for message in list %}
				<div class="flash flash-{{ cat }}">{{ message }}</div>
			{% endfor %}
		{% endfor %}
		{% block footer %}
			{{ include('partials/_footer.html.twig') }}
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</body>

</html>
